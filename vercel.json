{
  "version": 2,
  "name": "jp-morgan-training",
  "builds": [
    {
      "src": "jp-custom-trainer.html",
      "use": "@vercel/static"
    },
    {
      "src": "custom-manifest.json",
      "use": "@vercel/static"
    },
    {
      "src": "custom-sw.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "custom-sw.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "custom-manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ]
}
